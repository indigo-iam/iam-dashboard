{
  "name": "iam-dashboard",
  "private": true,
  "version": "0.1.0",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "scripts": {
    "dev": "next dev -p 8080",
