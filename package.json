  },
  "dependencies": {
    "@headlessui/react": "^2",
    "@heroicons/react": "^2",
    "next": "^14",
    "next-auth": "^5.0.0-beta",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "tailwindcss": "^3",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
